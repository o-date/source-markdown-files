	Shorttitle = {Archaeology 2.0},
	Title = {Archaeology 2.0: {New} {Approaches} to {Communication} and {Collaboration}},
	Url = {http://escholarship.org/uc/item/1r6137tb},
	Urldate = {2017-02-23},
	Year = {2011},
	Bdsk-Url-1 = {http://escholarship.org/uc/item/1r6137tb}}
