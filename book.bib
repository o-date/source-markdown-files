@article{seguin_digging_2017,
	Abstract = {Archaeology permits us precious glimpses into the mysterious past, captivating the public with its intrigue. However, the expensive nature of archaeological work often leads to a struggle for funding. Crowdfunding allows us to leverage archaeology's popular appeal to find the funds we have long struggled (and often failed) to obtain through traditional means. Furthermore, it lets us interact with the public in a new and novel way, allowing the different voices and narratives of the public to be heard. This thesis explores some of the benefits of crowdfunding. As well, data on crowdfunded archaeology projects were collected and analyzed and certain key success metrics were identified. Using these, a basic framework was developed to aid potential archaeological crowd funders. Considerations such as platform, media elements, and language, are an important part of the process. When used strategically, this tool can help unearth much for both the archaeologist and the public.},
	Author = {Seguin, Matthew J.},
	File = {Full Text PDF:/Users/shawngraham/Library/Application Support/Zotero/Profiles/rcpe5jts.default/zotero/storage/EX3FQSDN/Seguin - 2017 - Digging Online Crowdfunding for Archaeology.pdf:application/pdf;Snapshot:/Users/shawngraham/Library/Application Support/Zotero/Profiles/rcpe5jts.default/zotero/storage/2EGSSXI7/11181.html:text/html},
	Language = {en},
	Month = jan,
	Shorttitle = {Digging {Online}},
	Title = {Digging {Online}: {Crowdfunding} for {Archaeology}},
